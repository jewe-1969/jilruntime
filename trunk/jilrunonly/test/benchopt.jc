option "optimize=3, stack-locals=yes";

/*** Ack ***/

function int Ack_o(int M, int N)
{
    if (!M)
        return ++N;

    if (!N)
        return Ack_o(--M, 1);

    return Ack_o(M-1, Ack_o(M, --N));
}

/*** fib ***/

function int fib_o(int n)
{
    if (n < 2)
        return n;
    return fib_o(n-1) + fib_o(n-2);
}

/*** bench ***/

function int bench_o()
{
    int n=18;
    int x=0;
    for (int a=0;a<n;a++)
    {
        for (int b=0;b<n;b++)
        {
            for (int c=0;c<n;c++)
            {
                for (int d=0;d<n;d++)
                {
                    for (int e=0;e<n;e++)
                    {
                        for (int f=0;f<n;f++)
                        {
                            x++;
                        }
                    }
                }
            }
        }
    }
    return x;
}
